								"id": "3a7154c9-dfed-49b7-bad1-24716b36cdb4",
								"exec": [
									"var JsonResponse = JSON.parse(request.data);",
									"",
									"pm.test(\"Status code is 200\", function () {",
									"    var statusSucess = pm.response.to.have.status(200);",
									"    ",
									"    if(statusSucess) {",
									"        const echoPostRequest = {",
									"          url: pm.environment.get(\"URL\") + 'api/usuario/auth',",
									"          method: 'POST',",
									"          header: 'Content-Type:application/json',",
									"          body: {",
									"            mode: 'raw',",
									"            raw: JSON.stringify({ email: JsonResponse.email, senha: JsonResponse.senha })",
									"          }",
									"        };",
									"",
									"        pm.sendRequest(echoPostRequest, function (err, response) {",
									"            var jsonData = response.json();",
									"            postman.setEnvironmentVariable(\"access_token\", jsonData.token);",
									"        });",
									"    }",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
